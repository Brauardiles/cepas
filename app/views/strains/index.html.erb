<p id="notice"><%= notice %></p>

<h1>Strains</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @strains.each do |strain| %>
        <tr>
          <td><%= strain.name %></td>
          <td>
            <%= link_to 'Show', strain %>
            <%= link_to 'Edit', edit_strain_path(strain) %>
            <%= link_to 'Destroy', strain, method: :delete, data: { confirm: 'Are you sure?' } %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>



<br>

<%= link_to 'New Strain', new_strain_path, class:"btn btn-primary" %>
